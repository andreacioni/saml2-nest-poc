{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/saml2-nest-poc/src",
  "projects": {
    "security": {
      "type": "library",
      "root": "libs/security",
      "entryFile": "index",
      "sourceRoot": "libs/security/src",
      "compilerOptions": {
        "tsConfigPath": "libs/security/tsconfig.lib.json"
      }
    },
    "saml2-nest-poc": {
      "type": "application",
      "root": "apps/saml2-nest-poc",
      "entryFile": "main",
      "sourceRoot": "apps/saml2-nest-poc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/saml2-nest-poc/tsconfig.app.json"
      }
    },
    "home": {
      "type": "application",
      "root": "apps/home",
      "entryFile": "main",
      "sourceRoot": "apps/home/src",
      "compilerOptions": {
        "tsConfigPath": "apps/home/tsconfig.app.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/saml2-nest-poc/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/saml2-nest-poc"
}
